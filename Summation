import java.lang.*;
import java.util.*;

public class Sumnumber{
	public static void main(String args[]){
		Scanner s = new Scanner(System.in);
		int  a,b,c;
		System.out.println("Enter Two Number");
		a = s.nextInt();
		b = s.nextInt();
		c = a + b;
		System.out.println("Sum is " + c);
		
	}
	
}		
